{
  "name": "mutambo-api",
  "version": "0.0.4",
  "main": "index.js",
  "repository": "git@github.com:waterstone15/mutambo-api.git",
  "author": "Brian Waterstone <code@waterstone15.com>",
  "license": "The Unlicense",
  "private": true,
  "engines": {
    "node": "18",
    "yarn": "1"
  },
  "scripts": {
    "build": "yarn compile",
    "clean": "rm -rf ./dist",
    "compile": "run-s compile:coffee compile:static",
    "compile:coffee": "./node_modules/coffeescript/bin/coffee --compile --no-header --output ./dist ./",
    "compile:static": "cp -R ./img ./dist",
    "deploy:fly:prod": "flyctl deploy . --app mutambo-api",
    "deploy:prod": "run-s clean compile deploy:fly:prod",
    "s": "yarn serve",
    "serve": "run-s clean compile serve:watch",
    "serve:watch": "run-p serve:watch:coffee serve:watch:js",
    "serve:watch:coffee": "coffee --watch --compile --no-header --output ./dist ./",
    "serve:watch:js": "nodemon --delay 10ms ./dist/index.js --watch . --ext js,coffee,json,env,pug --ignore node_modules,scripts,.git",
    "start": "node ./dist/index.js",
    "stripe:proxy": "export $(grep DEV_SCOPE .env | xargs) && export $(grep PORT .env | xargs) && ngrok http -hostname=mutambo-stripe-$DEV_SCOPE.ngrok.io $PORT",
    "test": "mocha --require coffeescript/register --reporter list ./tests/**/*.coffee -t 60000"
  },
  "_moduleAliases": {
    "@": "./dist"
  },
  "dependencies": {
    "@firebase/app-compat": "0.x",
    "@firebase/app-types": "0.x",
    "@koa/cors": "^3.1.0",
    "@logtail/node": "^0.1.10",
    "@stablelib/base64": "^1.0.1",
    "@stablelib/binary": "^1.0.1",
    "@stablelib/hex": "^1.0.1",
    "@stablelib/random": "^1.0.1",
    "@stablelib/sha256": "^1.0.1",
    "@stablelib/sha512": "^1.0.1",
    "@stablelib/utf8": "^1.0.1",
    "@vvo/tzdb": "^6.7.0",
    "accounting": "^0.4.1",
    "algoliasearch": "^4.4.0",
    "base-64": "^1.0.0",
    "basic-auth": "^2.0.1",
    "chai": "^4.2.0",
    "coffeescript": "^2.6.1",
    "currency.js": "^2.0.3",
    "delay": "^4.4.0",
    "disposable-email-domains": "^1.0.57",
    "dotenv": "^10.0.0",
    "email-addresses": "^3.1.0",
    "faker": "^5.1.0",
    "firebase": "^8.2.0",
    "firebase-admin": "^10.0.1",
    "flame-lib": "^5.7.x",
    "google-libphonenumber": "^3.2.18",
    "got": "^11.8.1",
    "handlebars": "^4.7.6",
    "hash.js": "^1.1.7",
    "imap-simple": "^5.0.0",
    "js-nacl": "^1.3.2",
    "json2csv": "^5.0.6",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-favicon": "^2.1.0",
    "koa-helmet": "^6.1.0",
    "koa-logger": "^3.2.1",
    "koa-respond": "^2.1.0",
    "koa-router": "^10.1.1",
    "lodash": "^4.17.21",
    "luxon": "^2.3.0",
    "mocha": "^8.1.3",
    "module-alias": "^2.2.2",
    "moment-timezone": "^0.5.31",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "pluralize": "^8.0.0",
    "postmark": "^2.8.1",
    "pug": "^3.0.2",
    "qs": "^6.9.4",
    "rsvp": "^4.8.5",
    "stripe": "^8.67.0",
    "tsscmp": "^1.0.6",
    "tweetnacl": "^1.0.3",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  }
}
